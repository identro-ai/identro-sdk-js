{
  "name": "@identro/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Identro - AI Agent Credit Scoring",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./identity": {
      "types": "./dist/identity.d.ts",
      "import": "./dist/identity.mjs",
      "require": "./dist/identity.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build",
    "release": "changeset publish"
  },
  "keywords": [
    "identro",
    "ai",
    "agent",
    "scoring",
    "monitoring",
    "mcp",
    "langchain"
  ],
  "author": "Identro Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/identro-ai/identro-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/identro-ai/identro-sdk-js/issues"
  },
  "homepage": "https://github.com/identro-ai/identro-sdk-js#readme",
  "devDependencies": {
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@noble/ed25519": "^2.3.0",
    "@scure/base": "^1.2.6",
    "zod": "^4.0.10"
  }
}
